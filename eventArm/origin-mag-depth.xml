<eventArm>
    <fdsnEvent> <!-- default use USGS catalog -->
        <originTimeRange>
            <startTime>2010-05-19T00:00:00.000Z</startTime>
            <endTime>2010-05-20T00:00:00.000Z</endTime>
        </originTimeRange>
        <magnitudeRange>
            <min>5.8</min>
            <max>8.0</max>
        </magnitudeRange>
        <originDepthRange>
            <unit>KILOMETER</unit>
            <min>30</min>
        </originDepthRange>
    </fdsnEvent>
    <printlineEventProcess/>
    <CSVEventPrinter> <!-- save catalog to csv file -->
        <filename>events.csv</filename>
    </CSVEventPrinter>
</eventArm>
